@model DotNetCoreMVC.Models.Branch
@{
    ViewData["Title"] = "Contact";
}

<form asp-action="Contact" class="form-horizontal">
    <div class="row">
        <div class="col-md-3">
            <label class="control-label">BranchId</label>
            <input asp-for="BranchId" class="form-control" readonly="readonly" />
        </div>
        <div class="col-md-3">
            <label class="control-label">BranchName</label>
            <input asp-for="BranchName" class="form-control" />
        </div>
        <div class="col-md-3">
            <label class="control-label">Cluster</label>
            <input asp-for="Cluster" class="form-control" />
        </div>
        <div class="col-md-3">
            <label class="control-label">State</label>
            <input asp-for="State" class="form-control" />
        </div>
    </div>
    <div class="row">
        <div class="col-md-3">
            <label class="control-label">Status</label>
            <input asp-for="Status" class="form-control" />
        </div>
        <div class="col-md-3">
            <label class="control-label">CollectionBranch</label>
            <input asp-for="CollectionBranch" class="form-control" />
        </div>
        <div class="col-md-3">
            <label class="control-label">Tier</label>
            <input asp-for="Tier" class="form-control" />
        </div>
        <div class="col-md-3">
            <label class="control-label">Region</label>
            <input asp-for="Region" class="form-control" />
        </div>
    </div>
    <div class="row">
        <div class="col-md-3">
            <label class="control-label">Zone</label>
            <input asp-for="Zone" class="form-control" />
        </div>
    </div>
    <br />
    <div class="form-group">
        <input type="submit" value="Submit" class="btn btn-primary" />
    </div>

</form>

